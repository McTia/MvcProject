{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LiveReload": {
    "LiveReloadEnabled": true,
    "ClientFileExtensions": ".cshtml,.razor,.css,.js,.htm,.html",
    "ServerRefreshTimeout": 500,
    "WebSocketUrl": "/__livereload",
    "WebSocketHost": "wss://localhost:44368", /// NB la porta deve essere la stessa utilizzata da lochalost all'avvio dell app senza live reload!!!!
    "FolderToMonitor": "~/"
  }
}
